@{
    Layout = "_LayoutLogin";
}
@model Impulse.Model.SignUp
<div class="impulse-wrapper">
    <div class="imp-login">
        <div class="imp-login-table">
            <div class="imp-login-cell">
                <div class="container">
                    <div class="row">
                        <div class="imp-login-flex">
                            <div class="imp-login-box imp-login-left">
                                <div class="imp-login-vert-flex">
                                    <div class="imp-login-box-v imp-login-box-top">
                                        <h3 style="color: rgb(255, 255, 255);">An Impulse Initiative</h3>
                                        <p>iWAY is a platform that bridges tha gap between consumers and sports vendors to manage a sports event anywhere</p>
                                    </div>
                                    <div class="imp-login-box-v imp-login-box-bottom">

                                        <img src="~/images/login.png" alt="Login" />
                                    </div>
                                </div>
                            </div>
                            <div class="imp-login-box imp-login-right">
                                <div class="imp-login-form-wrap">
                                    <a>
                                        <div class="left-100 text-center">
                                            <img src="data:image/png;base64," alt="Impulse">
                                            @Html.ValidationMessage("successMsg")

                                        </div>
                                    </a>
                                    <div class="imp-login-form">
                                        <span id="para"></span>
                                        <form method="post" class="imp-login-form" id="SignUpFormVendor" asp-action="ICitizenSignUp">
                                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                            <div class="left-100">
                                                <input asp-for="Company" class="form-control" placeholder="Full Name" value="" />
                                                <span asp-validation-for="Company" class="text-danger"></span>
                                            </div>
                                            <div class="left-100">
                                                <input asp-for="Email" class="form-control" placeholder="Email" value="" />
                                                <span asp-validation-for="Email" class="text-danger"></span>
                                            </div>
                                            <div class="left-100">
                                                <input asp-for="Password" class="form-control" placeholder="Password" value="" />
                                                <span asp-validation-for="Password" class="text-danger"></span>
                                            </div>
                                            <div class="left-100">
                                                <input asp-for="ConfirmPassword" class="form-control" placeholder="Confirm Password" value="" />
                                                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                            </div>
                                            <div class="left-100">
                                                <input asp-for="Phone" class="form-control" placeholder="Phone" value="" />
                                                <span asp-validation-for="Phone" class="text-danger"></span>
                                            </div>
                                            <div class="left-100 imp-login-btns">
                                                <a class="pull-left" href="~/User/Login">LOGIN</a>
                                                <button type="submit" class="ant-btn imp-solid-button imp-button-small pull-right ant-btn-primary"><span>Register</span></button>
                                                @*<a class="pull-left">LOGIN</a>
                                                    <input type="submit" value="Create" class="ant-btn imp-solid-button imp-button-small pull-right ant-btn-primary" />*@
                                                @*<button>sign</button>*@
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><div class="imp-login-footer">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-lg-12">
                        <p>© letsimpulse 2018. All rights reserved.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
<script src="~/js/Login.js"></script>



